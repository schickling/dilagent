{
  "name": "dilagent",
  "version": "0.0.9",
  "type": "module",
  "bin": {
    "dilagent": "./dist/dilagent"
  },
  "files": [
    "src",
    "scripts",
    "dist"
  ],
  "scripts": {
    "build": "tsc --build && pnpm bundle",
    "bundle": "bun build --target node --outfile dist/dilagent src/main.ts && chmod +x dist/dilagent",
    "dev": "bun src/main.ts",
    "test": "vitest",
    "prepublishOnly": "pnpm build"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@effect/ai": "^0.26.1",
    "@effect/cli": "^0.69.2",
    "@effect/platform": "^0.90.7",
    "@effect/platform-node": "^0.96.1",
    "@effect/printer": "^0.45.0",
    "@effect/printer-ansi": "^0.45.0",
    "@effect/typeclass": "^0.36.0",
    "effect": "^3.17.13",
    "@types/node": "^24.3.1",
    "vitest": "^3.2.4"
  }
}
